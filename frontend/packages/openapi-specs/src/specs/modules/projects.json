{
  "openapi": "3.0.0",
  "info": {
    "title": "Projects API",
    "version": "1.0.0",
    "description": "Project management API endpoints"
  },
  "paths": {
    "/apis/v1/manager/system-config/projects/": {
      "get": {
        "summary": "Get All Projects",
        "description": "Get all projects with optional pagination and filtering",
        "tags": ["Projects"],
        "parameters": [
          {
            "name": "skip",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 0
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 100
            }
          },
          {
            "name": "name",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PaginatedAPIResponseProjectList"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create Project",
        "description": "Create a new project",
        "tags": ["Projects"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateProjectPayload"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Project Created Successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/APIResponseProject"
                }
              }
            }
          }
        }
      }
    },
    "/apis/v1/manager/system-config/projects/import": {
      "post": {
        "summary": "Import Projects From Csv",
        "description": "Import projects from CSV file",
        "tags": ["Projects"],
        "requestBody": {
          "required": true,
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary",
                    "description": "CSV file containing project data"
                  }
                },
                "required": ["file"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Import Successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/APIResponseString"
                }
              }
            }
          }
        }
      }
    },
    "/apis/v1/manager/system-config/projects/{project_id}": {
      "delete": {
        "summary": "Delete Project By Id",
        "description": "Delete a project by ID",
        "tags": ["Projects"],
        "parameters": [
          {
            "name": "project_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The ID of the project to delete"
          }
        ],
        "responses": {
          "200": {
            "description": "Deletion Successful",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/APIResponseBoolean"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Project": {
        "type": "object",
        "properties": {
          "_id": {
            "type": "string",
            "description": "MongoDB document ID"
          },
          "project_id": {
            "type": "string",
            "description": "Project ID"
          },
          "name": {
            "type": "string",
            "description": "Project name"
          },
          "is_active": {
            "type": "boolean",
            "description": "Whether the project is active",
            "default": true
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "description": "Creation timestamp"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time",
            "description": "Last update timestamp"
          }
        },
        "required": ["project_id", "name"]
      },
      "CreateProjectPayload": {
        "type": "object",
        "properties": {
          "project_id": {
            "type": "string",
            "description": "Project ID"
          },
          "name": {
            "type": "string",
            "description": "Project name"
          }
        },
        "required": ["project_id", "name"]
      },
      "APIResponseProject": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "$ref": "#/components/schemas/Project"
          }
        }
      },
      "APIResponseBoolean": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "boolean"
          }
        }
      },
      "APIResponseString": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "string"
          }
        }
      },
      "PaginatedAPIResponseProjectList": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Project"
            }
          },
          "limit": {
            "type": "integer"
          },
          "skip": {
            "type": "integer"
          },
          "total": {
            "type": "integer"
          }
        }
      }
    }
  }
}
